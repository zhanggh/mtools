<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
          http://www.springframework.org/schema/beans 
          http://www.springframework.org/schema/beans/spring-beans.xsd
          http://www.springframework.org/schema/context 
          http://www.springframework.org/schema/context/spring-context.xsd
          http://www.springframework.org/schema/mvc 
          http://www.springframework.org/schema/mvc/spring-mvc.xsd
          "
	default-lazy-init="true">

	<bean id="webConfigurer"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="localOverride" value="true" />
		<property name="locations">
			<list>
				<value>classpath:mtools.params.properties</value>
				<value>classpath:mtools.default.properties</value>
				<value>classpath:db.params.properties</value>
				<value>classpath:web.params.properties</value>
			</list>
		</property>
	</bean>
	<import resource="classpath:core-config.xml" />
	<import resource="classpath:app-service.xml" />

</beans>
